define [], () ->
	#to use this, make sure Ember-Data is already available
	me = 
		version: '{{version}}'
		models: '{{models}}'
		notify: ->
			alert 'Hello World!'
		attach: (App, names) ->
			for name in names
				App[name] = @models[name].model
				App[name + 'Adapter'] = @models[name].adapter
				App[name + 'Serializer'] = @models[name].serializer
			return
